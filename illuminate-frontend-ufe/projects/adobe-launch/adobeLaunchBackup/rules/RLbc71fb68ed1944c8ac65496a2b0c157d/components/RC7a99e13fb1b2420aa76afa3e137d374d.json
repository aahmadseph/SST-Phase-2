        "settings": "{\"source\":\"Sephora.analytics.promises.initialPageLoadFired.then(function() {\\n    var cost = _satellite.getVar('[TMS] Double Click :: Product Page :: Sku Price - dc - pinterest');\\n    var currencyCost = _satellite.getVar('[TMS] Double Click :: Product Page :: Complete Sku Price') || '0.00';\\n    var productId = _satellite.getVar('[TMS] Double Click :: Product Page :: Sku Id');\\n    var productType = _satellite.getVar('[TMS] Double Click :: Product Page :: Product Type');\\n    let eventID = _satellite.getVar('[UEID] :: Universal Event ID Fx')(event);\\n  \\n    if (currencyCost.indexOf('Points') \\u003c 0 \\u0026\\u0026 productType === 'Standard' \\u0026\\u0026 cost \\u003e 0) {\\n        pintrk('track', 'PageVisit', {\\n            value: cost,\\n            currency: 'USD',\\n            line_items: [{product_id: productId}],\\n            event_id: 'viewcontent-' + eventID\\n        });\\n    }\\n});\",\"language\":\"javascript\"}"
