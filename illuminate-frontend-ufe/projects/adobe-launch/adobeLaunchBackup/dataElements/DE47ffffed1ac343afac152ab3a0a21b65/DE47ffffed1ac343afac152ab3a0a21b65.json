        "clean_text": false,
        "default_value": null,
        "delegate_descriptor_id": "core::dataElements::custom-code",
        "force_lower_case": false,
        "review_status": "unsubmitted",
        "storage_duration": null,
        "settings": "{\"source\":\"return function parseProductString(productString) {\\n  \\n    productString = productString || '';\\n  \\n    // Split the product string by commas to separate multiple products\\n    const productsArray = productString.split(',');\\n    \\n    // Map over the array to create an array of product objects\\n    return productsArray.map(productEntry =\\u003e {\\n        const productDetails = productEntry.split(';');\\n\\n        // Extract custom events and merchandising eVars if they exist\\n        const customEvents = productDetails[4] ? productDetails[4].split('|') : [];\\n        const merchandisingEVars = productDetails[5] ? productDetails[5].split('|') : [];\\n\\n        return {\\n            category: productDetails[0] || '',\\n            product: productDetails[1] || '',\\n            quantity: productDetails[2] || '1',\\n            totalPrice: productDetails[3] || '0',\\n            customEvents: customEvents,\\n            merchandisingEVars: merchandisingEVars\\n        };\\n    });\\n}\"}"
    },
