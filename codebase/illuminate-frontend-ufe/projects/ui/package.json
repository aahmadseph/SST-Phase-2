{
    "name": "ui",
    "description": "Sephora.com Web UI",
    "version": "1.0.0",
    "private": true,
    "main": "js/app.js",
    "scripts": {
        "build": "node --max_old_space_size=2668 ../../node_modules/.bin/webpack --output-pathinfo",
        "build-aas-old-test": "AGENT_AWARE_BUILD_MODE=true BUILD_OPTIMIZATIONS=true BUILD_MODE=frontend node ../../node_modules/.bin/webpack --watch --progress",
        "build-analyse": "ANALYSE=true webpack --profile --json > logs/webpack.json",
        "build-frontend-info": "BUILD_OPTIMIZATIONS=true BUILD_MODE=frontend node ../../node_modules/.bin/webpack --watch --output-pathinfo --progress",
        "build-frontend-no-map": " NO_SOURCE_MAP=true npm run build-frontend",
        "build-jenkins-aas": "AGENT_AWARE_BUILD_MODE=true node --max_old_space_size=2668 ../../node_modules/.bin/webpack --output-pathinfo",
        "build-watch-no-map": "NO_SOURCE_MAP=true node ../../node_modules/.bin/webpack --watch --output-pathinfo --progress",
        "build-watch": "node ../../node_modules/.bin/webpack --watch --output-pathinfo --progress",
        "build-aas-dev": "AGENT_AWARE_BUILD_MODE=true BUILD_OPTIMIZATIONS=true npm run build --workspace=ui -- --progress",
        "build-debug-frontend": "BUILD_OPTIMIZATIONS=true BUILD_MODE=frontend node --inspect --inspect-brk ../../node_modules/webpack/bin/webpack.js --watch --output-pathinfo --progress",
        "build-debug": "node debug ../../node_modules/webpack/bin/webpack.js",
        "build-frontend": "BUILD_OPTIMIZATIONS=true BUILD_MODE=frontend node ../../node_modules/.bin/webpack --watch --progress",
        "clean": "rm -rf node_modules && rm -rf ./dist && rm -rf ./src/pages/index.js && rm -rf ./src/utils/locales/allResources.js && rm -rf ./__tests__/coverage",
        "eslint": "eslint 'src/**/*.{js,jsx}'",
        "eslint-modified": "eslint $(git diff --name-only --cached -- 'src/**/*.{js,jsx}')",
        "test": "NODE_OPTIONS='--no-deprecation' jest",
        "test-jasmine": "NO_SOURCE_MAP=true TEST_BUILD=true TEST_INSTRUMENTATION=true karma start karma.conf.js --single-run",
        "test-ui": "TEST_INSTRUMENTATION=true npm run test-no-map --max_old_space_size=2668",
        "test-dev": "TEST_BUILD=true BUILD_OPTIMIZATIONS=true BUILD_MODE=frontend karma start karma.conf.js",
        "test-dev-no-map": "NO_SOURCE_MAP=true npm run test-dev --workspace=ui",
        "test-no-map": "NO_SOURCE_MAP=true npm run test-watch -- --single-run",
        "test-watch": "TEST_BUILD=true BUILD_OPTIMIZATIONS=true BUILD_MODE=frontend karma start karma.conf.js",
        "update-gql-operations": "node tools/update-gql-operations.mjs",
        "storybook": "npx storybook dev -p 6006 --config-dir ./config/storybook",
        "storybook-clear-cache": "rm -rf node_modules/.cache",
        "build-storybook": "npx storybook build --config-dir ./config/storybook"
    },
    "engines": {
        "node": "22.12.0",
        "npm": "10.9.0"
    },
    "dependencies": {
        "@artsy/fresnel": "6.2.1",
        "@babel/register": "7.21.0",
        "@contentful/rich-text-react-renderer": "^16.0.1",
        "@emotion/cache": "11.13.1",
        "@emotion/css": "11.13.0",
        "@emotion/is-prop-valid": "1.3.0",
        "@emotion/react": "11.13.0",
        "@emotion/server": "11.11.0",
        "@emotion/styled": "11.13.0",
        "core-js": "3.30.1",
        "create-react-class": "15.6.3",
        "credit-card-type": "5.0.4",
        "deep-equal": "2.2.3",
        "focus-trap-react": "10.2.3",
        "jsbarcode": "^3.11.0",
        "lodash.get": "^4.4.2",
        "marked": "0.5.1",
        "prop-types": "15.8.1",
        "rc-slider": "8.6.1",
        "react": "16.14.0",
        "react-copy-to-clipboard": "5.1.0",
        "react-dom": "16.14.0",
        "react-google-recaptcha": "3.1.0",
        "react-hammerjs": "1.0.1",
        "react-popper": "^2.2.5",
        "react-qr-code": "2.0.15",
        "react-redux": "7.2.2",
        "redux": "3.5.2",
        "redux-action-watch": "1.0.6",
        "redux-thunk": "2.1.0",
        "redux-watch": "1.1.1",
        "reselect": "4.0.0",
        "smoothscroll-polyfill": "0.4.3",
        "styled-system": "^5.1.5",
        "whatwg-fetch": "1.0.0"
    },
