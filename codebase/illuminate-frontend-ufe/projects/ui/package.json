        "karma-parallel": "0.3.1",
        "karma-sourcemap-loader": "0.4.0",
        "karma-spec-reporter": "0.0.36",
        "karma-webpack": "5.0.0",
        "msw": "2.8.4",
        "object-assign": "4.1.0",
        "prettier": "3.5.3",
        "storybook": "7.6.20",
        "stream-browserify": "3.0.0",
        "thread-loader": "4.0.2",
        "webpack": "5.88.2",
        "webpack-bundle-analyzer": "4.10.2",
        "webpack-cli": "5.1.4",
        "webpack-manifest-plugin": "5.0.0"
    }
}
