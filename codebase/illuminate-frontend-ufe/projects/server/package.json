{
    "name": "server",
    "description": "JERRI/UFE/Woody servers",
    "version": "1.0.0",
    "private": true,
    "imports": {
        "#server/*": "./src/*",
        "#tests/*": "./tests/*"
    },
    "main": "index.mjs",
    "scripts": {
        "clean": "rm -rf node_modules",
        "build": "tools/build-server-docker-images.sh",
        "test": "tools/server-tests.sh",
        "eslint": "eslint --ext .js,.mjs src",
        "run-dev": "tools/router.sh --start --port=10443",
        "run-dev-frontend": "npm run run-dev --workspace=server -- --frontend",
        "run-dev-frontend-aas": "npm run run-dev --workspace=server -- --frontend --agent-aware",
        "start-auto-test": "tools/run.sh --qa --automation-targets",
        "start-debug": "tools/run.sh --noMemoize --workers=1 --logLevel=debug node --inspect",
        "start-develop": "tools/run.sh --noMemoize --workers=4 --frontend --automation-targets",
        "start-jerri-debug": "tools/router.sh --inspect --port=10443",
        "start-jerri-frontend": "tools/router.sh --frontend --port=10443",
        "start-jerri": "tools/router.sh --port=10443",
        "start-ufe": "tools/run.sh",
        "start-profile": "PROFILE=true tools/run.sh",
        "start-qa": "tools/run.sh --logLevel=debug --qa",
        "start-show-root": "tools/run.sh --noMemoize --workers=4 --frontend --automation-targets --show-root-comps",
        "start-silly": "tools/run.sh --logLevel=silly --automation-targets",
        "start-woody": "tools/router.sh --port=5000 --isWoody",
        "start-woody-debug": "tools/router.sh --port=5000 --isWoody --inspect",
        "start": "tools/run.sh --logLevel=debug --automation-targets --noMemoize",
        "stop": "tools/router.sh --stop --port=10443",
        "compose-isomode-embedded-down": "docker-compose -f ./docker-compose-iso-embedded.yaml down",
        "compose-isomode-embedded-up": ". tools/runProfiles/azr-${PROFILE_HOST}.profile.sh; DOCKER_DEFAULT_PLATFORM=linux/amd64 docker-compose -f ./docker-compose-iso-embedded.yaml up -d --scale base-docker-image=0 --build --force-recreate --remove-orphans",
        "compose-isomode-down": "docker-compose -f ./docker-compose-isomorphic.yaml down",
        "compose-isomode-up": ". tools/runProfiles/azr-${PROFILE_HOST}.profile.sh; DOCKER_DEFAULT_PLATFORM=linux/amd64 docker-compose -f ./docker-compose-isomorphic.yaml up -d --scale base-docker-image=0 --build --force-recreate --remove-orphans",
        "compose-jerri-ufe-down": "docker-compose -f ./docker-compose.yaml down",
        "compose-jerri-ufe-up": ". tools/runProfiles/azr-${PROFILE_HOST}.profile.sh; DOCKER_DEFAULT_PLATFORM=linux/amd64 docker-compose -f ./docker-compose.yaml up -d --scale base-docker-image=0 --build --force-recreate --remove-orphans",
        "docker-cleanup": "docker rmi `docker images -q`",
        "restart-debug": "npm run stop && npm run start-debug",
        "eslint-modified": "eslint --quiet `git status -s src | awk '{ if ($1 == \"R\") { print $4; } else if ($1 != \"D\") { print $2; } }' | grep -v '\\.sh$'`"
    },
    "engines": {
        "node": "22.12.0",
        "npm": "10.9.0"
    },
    "dependencies": {
        "@babel/register": "7.21.0",
        "@emotion/cache": "11.13.1",
        "@emotion/server": "11.11.0",
        "cookie-parser": "1.4.7",
        "express": "5.1.0",
        "global-agent": "3.0.0",
        "ioredis": "5.4.1",
        "node-jsx-babel": "0.14.2",
        "prom-client": "^15.0.0",
        "winston": "3.17.0"
    },
    "devDependencies": {
        "@babel/cli": "7.25.9",
        "@babel/core": "7.25.9",
        "@babel/eslint-parser": "7.25.9",
        "@babel/plugin-transform-class-properties": "7.25.9",
        "@babel/plugin-transform-object-rest-spread": "7.25.9",
        "@babel/plugin-transform-optional-chaining": "7.25.9",
        "@babel/preset-env": "7.25.9",
        "@babel/preset-react": "7.18.6",
        "@emotion/babel-preset-css-prop": "11.12.0",
        "autocannon": "8.0.0",
        "babel-loader": "8.3.0",
        "babel-plugin-transform-react-remove-prop-types": "0.4.24",
        "c8": "8.0.1",
        "chalk": "^2.4.1",
        "clinic": "13.0.0",
        "coverage-istanbul-loader": "3.0.5",
        "eslint": "8.57.1",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-plugin-disable": "2.0.3",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-ssr-friendly": "1.3.0",
        "espree": "9.6.1",
        "esprima": "4.0.1",
        "jasmine": "5.1.0",
        "jasmine-spec-reporter": "7.0.0",
        "js-beautify": "1.15.1",
        "prettier": "3.5.3",
        "stream-browserify": "3.0.0"
    }
}
