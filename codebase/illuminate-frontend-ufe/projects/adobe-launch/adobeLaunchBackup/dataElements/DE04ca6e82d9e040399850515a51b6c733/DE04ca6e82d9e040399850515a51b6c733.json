        "clean_text": false,
        "default_value": null,
        "delegate_descriptor_id": "core::dataElements::custom-code",
        "force_lower_case": false,
        "review_status": "unsubmitted",
        "storage_duration": null,
        "settings": "{\"source\":\"let priceToReturn = '';\\n  \\n// Verify the products exists within an array\\nif (digitalData.product !== null \\u0026\\u0026 Array.isArray(digitalData.product) \\u0026\\u0026 digitalData.product.length \\u003e 0) {\\n  priceToReturn = digitalData.product.map(p =\\u003e {\\n    let price ='0.00';\\n    if (p.attributes.price !== 'FREE') {\\n      price = p.attributes.price;\\n      price = price.match(/\\\\d+(?:\\\\.\\\\d+|\\\\,\\\\d+\\\\.\\\\d+|\\\\,\\\\d+)|\\\\d+/)[0];\\n      if (digitalData.page.pageInfo.language.toLowerCase() === 'fr') { \\n        price = price.replace(',', '.'); \\n      } else { \\n        price = price.replace(',', ''); \\n      }\\n      price = Sephora.analytics.utils.convertToUSD(price);\\n    }\\n    return price;\\n  }).join(',');\\n}\\n\\nreturn priceToReturn; \"}"
    },
