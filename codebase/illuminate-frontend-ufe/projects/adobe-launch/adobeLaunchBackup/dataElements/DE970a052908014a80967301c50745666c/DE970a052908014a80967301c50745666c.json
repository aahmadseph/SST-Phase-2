        "storage_duration": null,
        "settings": "{\"source\":\"const locale = digitalData.page.attributes.languageLocale;\\n\\n// Transform and map the locale\\nconst transformedLocale = locale.toLowerCase().replace(/-/g, '_').replace(/(.*)_(.*)/, '$2_$1');\\n\\nconst finalLocale = (() =\\u003e {\\n  switch (transformedLocale) {\\n    case \\\"en_us\\\":\\n      return \\\"en_us\\\";\\n    case \\\"fr_ca\\\":\\n      return \\\"fr_ca\\\";\\n    case \\\"en_ca\\\":\\n      return \\\"en_ca\\\";\\n    default:\\n      return \\\"en_us\\\";\\n  }\\n})();\\n\\nreturn finalLocale;\\n\"}"
    },
