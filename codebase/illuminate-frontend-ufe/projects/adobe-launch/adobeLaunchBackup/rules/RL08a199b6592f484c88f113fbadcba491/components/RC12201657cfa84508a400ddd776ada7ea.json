        "updated_by_display_name": "roman zhelieznov",
        "settings": "{\"source\":\"(async function () {\\n    if (window.ZineOne || !Sephora.configurationSettings.isSessionAIEnabled) {\\n      return;\\n    }\\n  \\n    const apiKeysMap = {\\n      ['us_en']: Sephora.configurationSettings.sessionAISDKApiKeyUS,\\n      ['ca_en']: Sephora.configurationSettings.sessionAISDKApiKeyCAEN,\\n      ['ca_fr']: Sephora.configurationSettings.sessionAISDKApiKeyCAFR\\n    }\\n  \\n    const cookies = Sephora.Util.cookieStore() || {};\\n    const { site_locale = '', site_language = '' } = cookies;\\n  \\n    const sessionAIApiKey = apiKeysMap[`${site_locale}_${site_language}`];\\n  \\n    try {\\n        const response = await fetch('https://cdn.zineone.com/cloud/latest/z1m.js');\\n\\n        if (!response.ok) {\\n            console.error(`Session AI: Failed to load script: ${response.statusText}`);\\n            return;\\n        }\\n\\n        const scriptContent = await response.text();\\n\\n        const scriptElement = document.createElement('script');\\n        scriptElement.textContent = scriptContent;\\n        document.body.appendChild(scriptElement);\\n\\n        if (sessionAIApiKey) {\\n            window.ZineOne.initialize(sessionAIApiKey);\\n        }\\n    } catch (error) {\\n        console.error('Session AI: Error loading or executing script:', error);\\n    }\\n})();\",\"language\":\"javascript\"}"
    },
